    "solver": "SGD",
    "regularizer": "L2",
    "momentum": 0.9,
    "weight_decay": 0.0005,
    "base_lr": 0.001,
    "lr_policy": "LR_step",
    "lr_gamma": 0.9,
    "lr_stepsize": 10000,
    "path": "PeriodNet1",
    "snapshot_prefix": "PeriodNet1_snapshot",
    "max_iter": 100000,
    "snapshot_iter": 2000,
    "display_iter": 1,
    "test_iter": 5,
    "test_interval": 100,
    "GPU": 1
  },
  "test": {
    "GPU": 1
  },
  "layers": [
    {
      "type":"PeriodTrainData",
      "name":"dataTrain",
      "phase":"Training",
      "file_data":["../../data/train"],
      "batch_size":32,
      "size_crop":[30,30,30],
      "out":[
        "data2D",
        "data3D",
        "label_class",
        "label_axis",
        "label_angle",
        "label_axis_weights",
        "label_angle_weights"
      ]
    },
    {
      "type":"PeriodTrainData",
      "name":"dataTest",
      "phase":"Testing",
      "file_data":["../../data/test"],
      "batch_size":32,
      "size_crop":[30,30,30],
      "out":[
        "data2D",
        "data3D",
        "label_class",
        "label_axis",
        "label_angle",
        "label_axis_weights",
        "label_angle_weights"
