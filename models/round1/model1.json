    },
    {
      "in": ["Cfc2"],
      "type": "Dropout",
      "name": "Cdrop2",
      "dropout_ratio": 0.5,
      "out": ["Cfc2"]
    },
    {
      "in": ["Cfc2"],
      "type": "InnerProduct",
      "name": "fc_class",
      "num_output": 2,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.01,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 0,
      "train_me": true,
      "out": ["class_score"]
    },
    {
      "in": ["class_score"],
      "type": "Softmax",
      "stable_gradient": true,
      "name": "class_score",
      "out": ["class_score"]
    },
    {
      "in": [
        "class_score",
        "label_class"
      ],
      "type": "Loss",
      "name": "loss_class",
      "mode": "MultinomialLogistic_StableSoftmax",
      "loss_weight": 1
    }
  ]
}