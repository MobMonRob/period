        "window":[
            3,
            3
        ]
    },
    {
        "bias_decay_mult":0.0,
        "bias_filler":"Constant",
        "bias_filler_param":0.1,
        "bias_lr_mult":2.0,
        "group":2,
        "in":[
            "pool1"
        ],
        "name":"conv2",
        "train_me": false,
        "num_output":256,
        "out":[
