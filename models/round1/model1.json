      "weight_filler_param": 0.01,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 0,
      "train_me": true,
      "out": ["3Dconv3"]
    },
    {
      "in": ["3Dconv3"],
      "type": "Activation",
      "name": "3Drelu3",
      "mode": "ReLU",
      "out": ["3Dconv3"]
    },
    {
      "in": ["3Dconv3"],
      "type": "InnerProduct",
      "name": "3Dfc4",
      "num_output": 4096,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.01,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 1,
      "train_me": true,
      "out": ["3Dfc4"]
    },
    {
      "in": ["3Dfc4"],
      "type": "Activation",
      "name": "3Drelu4",
      "mode": "ReLU",
      "out": ["3Dfc4"]
    },
    {
      "in": ["3Dfc4"],
      "type": "Dropout",
      "name": "3Ddrop4",
      "dropout_ratio": 0.5,
      "out": ["3Dfc4"]
    },
    {
      "in": ["3Dfc4"],
      "type": "InnerProduct",
      "name": "3Dfc5",
      "num_output": 1000,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.005,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 1,
      "train_me": true,
      "out": ["3Dfc5"]
    },
    {
      "in": ["3Dfc5"],
      "type": "Activation",
      "name": "3Drelu5",
      "mode": "ReLU",
      "out":  ["3Dfc5"]
    },
    {
      "in": ["3Dfc5"],
      "type": "Dropout",
      "name": "3Ddrop5",
      "dropout_ratio": 0.5,
      "out": ["3Dfc5"]
    },
    {
      "in": ["3Dfc5"],
      "type": "InnerProduct",
      "name": "3Dfc6",
      "num_output": 1000,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.005,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 1,
      "train_me": true,
      "out": ["3Dfc6"]
    },

















    {
        "bias_decay_mult":0.0,
        "bias_filler":"Constant",
        "bias_filler_param":0.0,
        "bias_lr_mult":2.0,
        "in":[
            "data2D"
        ],
        "name":"conv1",
        "train_me": false,
        "num_output":96,
        "out":[
            "conv1"
        ],
        "padding":[
            0,
            0
        ],
        "stride":[
            4,
            4
        ],
        "type":"Convolution",
        "weight_decay_mult":1.0,
        "weight_filler":"Gaussian",
        "weight_filler_param":0,
        "weight_lr_mult":1.0,
        "window":[
            11,
            11
        ]
    },
    {
        "in":[
            "conv1"
        ],
        "mode":"ReLU",
        "name":"relu1",
        "out":[
            "conv1"
        ],
        "type":"Activation"
    },
    {
        "alpha":0.0001,
        "beta":0.75,
        "in":[
            "conv1"
        ],
        "k":1,
        "local_size":5,
        "name":"norm1",
        "out":[
            "norm1"
        ],
        "type":"LRN"
    },
    {
        "in":[
            "norm1"
        ],
        "mode":"max",
        "name":"pool1",
        "out":[
            "pool1"
        ],
        "padding":[
            0,
            0
        ],
        "stride":[
            2,
            2
        ],
