        "type":"InnerProduct",
        "weight_decay_mult":1.0,
        "weight_filler":"Gaussian",
        "weight_filler_param":0,
        "weight_lr_mult":1.0
    },






















    {
      "in":["3Dfc6","fc8"],
      "type":"Concat",
      "name":"ConcateLayer",
      "out":["combine"]
    },
    {
      "in": ["combine"],
      "type": "InnerProduct",
      "name": "Cfc1",
      "num_output": 1000,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.005,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 1,
      "train_me": true,
      "out": ["Cfc1"]
    },
    {
      "in": ["Cfc1"],
