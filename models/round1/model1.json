    {
      "in": ["data3D"],
      "type": "Convolution",
      "name": "3Dconv1",
      "num_output": 96,
      "window": [5,5,5],
      "padding": [1,1,1],
      "stride": [1,1,1],
      "upscale": [1,1,1],
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.01,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 0,
      "train_me": 1,
      "train_me":true,
      "out": ["3Dconv1"]

    },
    {
      "in": ["3Dconv1"],
      "type": "Activation",
      "name": "3Drelu1",
      "mode": "ReLU",
      "out": ["3Dconv1"]
    },
    {
      "in": ["3Dconv1"],
      "type": "Pooling",
      "name": "3Dpool1",
      "mode": "max",
      "window": [2,2,2],
      "padding": [0,0,0],
      "stride": [2,2,2],
      "out": ["3Dpool1"]
    },
    {
      "in": ["3Dpool1"],
      "type": "Convolution",
      "name": "3Dconv2",
      "num_output": 192,
      "window": [3,3,3],
      "stride": [1,1,1],
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.01,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 0,
      "train_me": 1,
      "train_me":true,
      "out": ["3Dconv2"]
    },
    {
      "in": ["3Dconv2"],
      "type": "Activation",
      "name": "3Drelu2",
      "mode": "ReLU",
      "out": ["3Dconv2"]
    },
    {
      "in": ["3Dconv2"],
      "type": "Pooling",
      "name": "3Dpool2",
      "mode": "max",
      "window": [2,2,2],
      "stride": [2,2,2],
      "out": ["3Dpool2"]
    },
    {
      "in": ["3Dpool2"],
