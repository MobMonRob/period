    },
    {
        "bias_decay_mult":0.0,
        "bias_filler":"Constant",
        "bias_filler_param":0.1,
        "bias_lr_mult":2.0,
        "group":2,
        "in":[
            "conv3"
        ],
        "name":"conv4",
        "train_me": false,
        "num_output":384,
        "out":[
            "conv4"
        ],
        "padding":[
            1,
            1
        ],
        "stride":[
            1,
            1
        ],
        "type":"Convolution",
        "weight_decay_mult":1.0,
        "weight_filler":"Gaussian",
        "weight_filler_param":0,
