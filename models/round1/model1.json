        "out":[
            "conv4"
        ],
        "type":"Activation"
    },
    {
        "bias_decay_mult":0.0,
        "bias_filler":"Constant",
        "bias_filler_param":0.1,
        "bias_lr_mult":2.0,
        "group":2,
        "in":[
            "conv4"
        ],
        "name":"conv5",
        "train_me": false,
        "num_output":256,
        "out":[
            "conv5"
        ],
        "padding":[
            1,
            1
        ],
        "stride":[
            1,
            1
        ],
        "type":"Convolution",
        "weight_decay_mult":1.0,
        "weight_filler":"Gaussian",
        "weight_filler_param":0,
        "weight_lr_mult":1.0,
        "window":[
            3,
            3
        ]
    },
    {
        "in":[
            "conv5"
        ],
        "mode":"ReLU",
        "name":"relu5",
        "out":[
            "conv5"
        ],
        "type":"Activation"
    },
    {
        "in":[
            "conv5"
        ],
        "mode":"max",
        "name":"pool5",
        "out":[
            "pool5"
        ],
        "padding":[
            0,
            0
        ],
        "stride":[
            2,
            2
        ],
        "type":"Pooling",
        "window":[
            3,
            3
        ]
    },
    {
        "bias_decay_mult":0.0,
