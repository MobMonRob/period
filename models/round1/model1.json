      "dropout_ratio": 0.5,
      "out": ["Cfc1"]
    },
    {
      "in": ["Cfc1"],
      "type": "InnerProduct",
      "name": "Cfc2",
      "num_output": 1000,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.005,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
