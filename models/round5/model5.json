{
  "train": {
    "solver": "SGD",
    "regularizer": "L2",
    "momentum": 0.9,
    "weight_decay": 0.0005,
    "base_lr": 0.01,
    "lr_policy": "LR_step",
    "lr_gamma": 0.9,
    "lr_stepsize": 10000,
    "path": "models/PeriodNet.5.5",
    "snapshot_prefix": "models/PeriodNet.5.5.",
    "max_iter": 100000,
    "snapshot_iter": 5000,
    "display_iter": 1,
    "test_iter": 5,
    "test_interval": 100,
    "GPU": 5
  },
  "test": {
    "GPU": 5
  },
  "layers": [
    {
      "type":"PeriodTrainData",
      "name":"dataTrain",
      "phase":"Training",
      "file_data":["data/train"],
      "batch_size":28,
      "size_crop":[30,30,30],
      "out":[
        "data2D",
        "data3D",
        "label_class",
        "label_quat",
        "label_quat_weights",
        "label_trans",
        "label_trans_weights"
      ]
    },
    {
      "type":"PeriodTrainData",
      "name":"dataTest",
      "phase":"Testing",
      "file_data":["data/train"],
      "batch_size":28,
      "size_crop":[30,30,30],
      "out":[
        "data2D",
        "data3D",
        "label_class",
        "label_quat",
        "label_quat_weights",
        "label_trans",
        "label_trans_weights"
      ]
    },
    {
      "in": ["data3D"],
      "type": "Convolution",
      "name": "3Dconv1",
      "num_output": 96,
      "window": [5,5,5],
      "padding": [1,1,1],
      "stride": [1,1,1],
      "upscale": [1,1,1],
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.01,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 0,
      "train_me": 1,
      "train_me":true,
      "out": ["3Dconv1"]

    },
    {
      "in": ["3Dconv1"],
      "type": "Activation",
      "name": "3Drelu1",
      "mode": "ReLU",
      "out": ["3Dconv1"]
    },
    {
      "in": ["3Dconv1"],
      "type": "Pooling",
      "name": "3Dpool1",
      "mode": "max",
      "window": [2,2,2],
      "padding": [0,0,0],
      "stride": [2,2,2],
      "out": ["3Dpool1"]
    },
    {
      "in": ["3Dpool1"],
      "type": "Convolution",
      "name": "3Dconv2",
      "num_output": 192,
      "window": [3,3,3],
      "stride": [1,1,1],
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.01,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 0,
      "train_me": 1,
      "train_me":true,
      "out": ["3Dconv2"]
    },
    {
      "in": ["3Dconv2"],
      "type": "Activation",
      "name": "3Drelu2",
      "mode": "ReLU",
      "out": ["3Dconv2"]
    },
    {
      "in": ["3Dconv2"],
      "type": "Pooling",
      "name": "3Dpool2",
      "mode": "max",
      "window": [2,2,2],
      "stride": [2,2,2],
      "out": ["3Dpool2"]
    },
    {
      "in": ["3Dpool2"],
      "type": "Convolution",
      "name": "3Dconv3",
      "num_output": 384,
      "window": [3,3,3],
      "stride": [1,1,1],
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.01,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 0,
      "train_me": true,
      "out": ["3Dconv3"]
    },
    {
      "in": ["3Dconv3"],
      "type": "Activation",
      "name": "3Drelu3",
      "mode": "ReLU",
      "out": ["3Dconv3"]
    },
    {
      "in": ["3Dconv3"],
      "type": "InnerProduct",
      "name": "3Dfc4",
      "num_output": 4096,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.01,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 1,
      "train_me": true,
      "out": ["3Dfc4"]
    },
    {
      "in": ["3Dfc4"],
      "type": "Activation",
      "name": "3Drelu4",
      "mode": "ReLU",
      "out": ["3Dfc4"]
    },
    {
      "in": ["3Dfc4"],
      "type": "Dropout",
      "name": "3Ddrop4",
      "dropout_ratio": 0.5,
      "out": ["3Dfc4"]
    },
    {
      "in": ["3Dfc4"],
      "type": "InnerProduct",
      "name": "3Dfc5",
      "num_output": 1000,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.005,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 1,
      "train_me": true,
      "out": ["3Dfc5"]
    },
    {
      "in": ["3Dfc5"],
      "type": "Activation",
      "name": "3Drelu5",
      "mode": "ReLU",
      "out":  ["3Dfc5"]
    },
    {
      "in": ["3Dfc5"],
      "type": "Dropout",
      "name": "3Ddrop5",
      "dropout_ratio": 0.5,
      "out": ["3Dfc5"]
    },
    {
      "in": ["3Dfc5"],
      "type": "InnerProduct",
      "name": "3Dfc6",
      "num_output": 1000,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.005,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 1,
      "train_me": true,
      "out": ["3Dfc6"]
    },
















    
    {
      "in": ["3Dfc6"],
      "type": "InnerProduct",
      "name": "fc_class",
      "num_output": 3,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.01,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 0,
      "train_me": true,
      "out": ["class_score"]
    },
    {
      "in": ["class_score"],
      "type": "Softmax",
      "stable_gradient": true,
      "name": "class_score",
      "out": ["class_score"]
    },
    {
      "in": [
        "class_score",
        "label_class"
      ],
      "type": "Loss",
      "name": "loss_class",
      "mode": "MultinomialLogistic_StableSoftmax",
      "loss_weight": 1
    },














    {
      "in": ["3Dfc4"],
      "type": "InnerProduct",
      "name": "Qfc1",
      "num_output": 1000,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.005,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 1,
      "train_me": true,
      "out": ["Qfc1"]
    },
    {
      "in": ["Qfc1"],
      "type": "Activation",
      "name": "Qrelu1",
      "mode": "ReLU",
      "out":  ["Qfc1"]
    },
    {
      "in": ["Qfc1"],
      "type": "Dropout",
      "name": "Qdrop1",
      "dropout_ratio": 0.5,
      "out": ["Qfc1"]
    },
    {
      "in": ["Qfc1"],
      "type": "InnerProduct",
      "name": "Qfc2",
      "num_output": 1000,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.005,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 1,
      "train_me": true,
      "out": ["Qfc2"]
    },
    {
      "in": ["Qfc2"],
      "type": "Activation",
      "name": "Qrelu2",
      "mode": "ReLU",
      "out":  ["Qfc2"]
    },
    {
      "in": ["Qfc2"],
      "type": "Dropout",
      "name": "Qdrop2",
      "dropout_ratio": 0.5,
      "out": ["Qfc2"]
    },
    {
      "in": ["Qfc2"],
      "type": "InnerProduct",
      "name": "fc_quat",
      "num_output": 8,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.01,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 0,
      "train_me": true,
      "out": ["quat_pred"]
    },
    {
      "in": [
        "quat_pred",
        "label_quat",
        "label_quat_weights"
      ],
      "type": "Loss",
      "name": "loss_quat",
      "mode": "EuclideanSSE",
      "loss_weight": 0.1
    },



















    {
      "in": ["3Dfc4"],
      "type": "InnerProduct",
      "name": "Tfc1",
      "num_output": 1000,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.005,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 1,
      "train_me": true,
      "out": ["Tfc1"]
    },
    {
      "in": ["Tfc1"],
      "type": "Activation",
      "name": "Trelu1",
      "mode": "ReLU",
      "out":  ["Tfc1"]
    },
    {
      "in": ["Tfc1"],
      "type": "Dropout",
      "name": "Tdrop1",
      "dropout_ratio": 0.5,
      "out": ["Tfc1"]
    },
    {
      "in": ["Tfc1"],
      "type": "InnerProduct",
      "name": "Tfc2",
      "num_output": 1000,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.005,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 1,
      "train_me": true,
      "out": ["Tfc2"]
    },
    {
      "in": ["Tfc2"],
      "type": "Activation",
      "name": "Trelu2",
      "mode": "ReLU",
      "out":  ["Tfc2"]
    },
    {
      "in": ["Tfc2"],
      "type": "Dropout",
      "name": "Tdrop2",
      "dropout_ratio": 0.5,
      "out": ["Tfc2"]
    },
    {
      "in": ["Tfc2"],
      "type": "InnerProduct",
      "name": "fc_trans",
      "num_output": 6,
      "weight_lr_mult": 1,
      "weight_decay_mult": 1,
      "weight_filler": "Gaussian",
      "weight_filler_param": 0.01,
      "bias_lr_mult": 2,
      "bias_decay_mult": 0,
      "bias_filler": "Constant",
      "bias_filler_param": 0,
      "train_me": true,
      "out": ["trans_pred"]
    },
    {
      "in": [
        "trans_pred",
        "label_trans",
        "label_trans_weights"
      ],
      "type": "Loss",
      "name": "loss_trans",
      "mode": "SmoothL1",
      "loss_weight": 1
    }





























  ]
}